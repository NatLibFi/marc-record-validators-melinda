{
  "name": "@natlibfi/marc-record-validators-melinda",
  "description": "MARC record validators used in Melinda",
  "type": "module",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://www.npmjs.com/package/@natlibfi/marc-record-validators-melinda",
  "bugs": {
    "url": "https://github.com/natlibfi/marc-record-validators-melinda/issues"
  },
  "repository": {
    "url": "https://github.com/NatLibFi/marc-record-validators-melinda"
  },
  "license": "MIT",
  "version": "12.0.0-alpha.7",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "esbuild \"src/**/*.js\" --sourcemap --platform=node --outdir=dist",
    "lint": "eslint ./src",
    "lint:dev": "eslint --fix ./src",
    "test": "npm run lint && npm run test:base",
    "test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
    "dev:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
    "dev:debug": "cross-env LOG_LEVEL=debug DEBUG=@natlibfi/* NODE_ENV=test"
  },
  "dependencies": {
    "@natlibfi/iso9-1995": "next",
    "@natlibfi/issn-verify": "^1.0.6",
    "@natlibfi/marc-record": "next",
    "@natlibfi/marc-record-serializers": "next",
    "@natlibfi/marc-record-validate": "next",
    "@natlibfi/melinda-commons": "next",
    "@natlibfi/sfs-4900": "next",
    "@natlibfi/sru-client": "next",
    "cld3-asm": "^4.0.0",
    "clone": "^2.1.2",
    "debug": "^4.4.3",
    "isbn3": "1.2.15",
    "langs": "^2.0.0",
    "undici": "^7.16.0",
    "xml2js": "^0.6.2",
    "xregexp": "^5.1.2"
  },
  "peerDependencies": {
    "@natlibfi/marc-record-validate": "next"
  },
  "devDependencies": {
    "@natlibfi/fixugen": "next",
    "@natlibfi/fixura": "next",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "fetch-mock": "^12.6.0"
  },
  "overrides": {
    "nanoid": "^3.3.8"
  },
  "nvolkComment": "cld3-asm 4.0.0 uses emscripten-wasm-loader ^3.0.3 which uses problematic, non-secure nanoid version 2.X.X. Isbn3 has breaking changes in 1.2.16-18 at least."
}
